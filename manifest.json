{
  "manifest_version": 2,
  "name": "SpanTree - GitLab Tree",
  "description": "Tree for Gitlab",
  "version": "0.0.1.0",
  "background": {
    "scripts": ["event.js"]
  },
  "permissions": ["<all_urls>"],
  "browser_action": {
    "default_title": "SpanTree"
  },
  "web_accessible_resources": ["*.png", "*.woff2", "*.svg"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["libs/file-icons.css"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  }
}
