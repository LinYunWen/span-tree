{
  "manifest_version": 2,
  "name": "SpanTree",
  "description": "Tree for Gitlab",
  "version": "0.0.0.1",
  "background": {
    "scripts": ["event.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "SpanTree",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["*.png", "*.woff2", "*.svg"],
  "content_scripts": [
    {
      "matches": ["https://gitlab.com/*/*", "https://gitlab.com/*/*/-/tree/*"],
      "css": [],
      "js": ["content.js"]
    }
  ]
}
