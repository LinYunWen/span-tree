{
  "manifest_version": 2,
  "name": "SpanTree",
  "description": "Tree for Gitlab",
  "version": "0.0.0.1",
  "background": {
    "scripts": ["event.js"],
    "persistent": true
  },
  "permissions": ["https://www.gitlab.com/"],
  "browser_action": {
    "default_title": "SpanTree",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["*.png", "*.woff2", "*.svg"],
  "content_scripts": [
    {
      "matches": ["https://gitlab.com/*/*", "https://gitlab.com/*/*/-/tree/*"],
      "css": ["libs/file-icons.css"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  }
}
